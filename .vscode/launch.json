    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: remote_debug",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "127.0.0.1",
                "port": 7389
            },
            "subProcess": true,
            "justMyCode": false,
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "subProcess": true,
        }
    ]
}